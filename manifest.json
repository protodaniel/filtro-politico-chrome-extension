{
  "name": "Sin Ruido Político",
  "version": "0.1",
  "description": "Remueve el ruido político de tu feed the Facebook",
  "background": {
    "scripts": ["bootstrap.js"],
    "persistent": false
  },
  "permissions": [
    "https://www.facebook.com/",
    "http://www.facebook.com/",
    "tabs",
    "storage"
  ],
  "icons": {
    "256": "images/256.png",
    "128": "images/128.png",
    "64": "images/64.png",
    "36": "images/36.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*.facebook.com/*", "https://*.facebook.com/*"],
      "js": ["libs/debounce.js", "libs/md5.js", "sinruido.js"]
    }
  ],
  "options_ui": {
    "open_in_tab": false,
    "page": "settings.html"
  },
  "manifest_version": 2
}
